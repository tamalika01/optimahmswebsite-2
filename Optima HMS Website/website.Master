    <script type="text/javascript">
        $(document).ready(function () {
            $("#submitForm1").click(function (event) {
                event.preventDefault(); // Prevent default form submission

                // Clear previous error messages
                $('.error-message').remove();

                var isValid = true; // Flag to check form validity

                // Fetch form values
                var name = $("#<%= txname.ClientID %>").val().trim();
            var phone = $("#<%= txphone.ClientID %>").val().trim();
            var email = $("#<%= txemail.ClientID %>").val().trim();
            var service = $("#<%= dlService.ClientID %> option:selected").text();
            var contactTime = $("#<%= dlCnctTym.ClientID %> option:selected").text();
            var message = $("#<%= txmessage.ClientID %>").val().trim();

            // Basic form validation
            if (name === "") {
                displayErrorMessage("<%= txname.ClientID %>", "Please enter your name.");
                isValid = false;
            }
            if (phone === "") {
                displayErrorMessage("<%= txphone.ClientID %>", "Please enter your phone number.");
                isValid = false;
            }
            if (email === "") {
                displayErrorMessage("<%= txemail.ClientID %>", "Please enter your email.");
                isValid = false;
            }
            if (service === "--SELECT--") {
                displayErrorMessage("<%= dlService.ClientID %>", "Please select a service.");
                isValid = false;
            }
            if (contactTime === "") {
                displayErrorMessage("<%= dlCnctTym.ClientID %>", "Please select a preferred time to contact.");
                isValid = false;
            }
            if (message === "") {
                displayErrorMessage("<%= txmessage.ClientID %>", "Please enter a message.");
                isValid = false;
            }

            if (isValid) {
                // If the form is valid, send the email via AJAX
                $.ajax({
                    type: "POST",
                    url: "default.aspx/SendEmail",
                    data: JSON.stringify({
                        name: name,
                        phone: phone,
                        email: email,
                        service: service,
                        contactTime: contactTime,
                        message: message
                    }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if (response.d === "success") {
                            alert("Email sent successfully.");
                            // Clear the form fields
                            clearFormFields();
                        } else {
                            alert("Failed to send email.");
                        }
                    },
                    error: function (xhr, status, error) {
                        alert("An error occurred: " + error);
                    }
                });
            }
        });

        // Function to display error messages
        function displayErrorMessage(elementId, message) {
            var element = $("#" + elementId);
            var errorMessage = $('<div class="error-message text-danger"></div>').text(message);
            element.after(errorMessage); // Insert error message after the input field
        }

        // Function to clear form fields and error messages
        function clearFormFields() {
            $("#<%= txname.ClientID %>").val('');
            $("#<%= txphone.ClientID %>").val('');
            $("#<%= txemail.ClientID %>").val('');
            $("#<%= dlService.ClientID %>").val('--SELECT--');
            $("#<%= dlCnctTym.ClientID %>").val('Any Time');
            $("#<%= txmessage.ClientID %>").val('');
            $('.error-message').remove(); // Clear error messages
        }
    });
    </script>


