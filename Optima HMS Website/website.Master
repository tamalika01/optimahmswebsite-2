    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $("#formSumbit").click(function (event) {
                event.preventDefault(); // Prevent the default form submission

                // Get field values
                var name = $("#txtn_name").val();
                var phone = $("#txtn_phone").val();
                var email = $("#txtn_email").val();
                var service = $("#ddl_Service option:selected").val();
                var contactTime = $("#ddl_CnctTym option:selected").val();
                var message = $("#txt_message").val();

                var hasError = false;

                // Clear previous error messages
                $(".error-msg").remove();

                // Validate each field and show specific error messages
                if (!name) {
                    $("#txtn_name").after('<span class="error-msg text-danger">Please enter your name.</span>');
                    hasError = true;
                }

                if (!phone) {
                    $("#txtn_phone").after('<span class="error-msg text-danger">Please enter your phone number.</span>');
                    hasError = true;
                }

                if (!email) {
                    $("#txtn_email").after('<span class="error-msg text-danger">Please enter your email.</span>');
                    hasError = true;
                }

                if (!service) {
                    $("#ddl_Service").after('<span class="error-msg text-danger">Please select a service.</span>');
                    hasError = true;
                }

                if (!contactTime) {
                    $("#ddl_CnctTym").after('<span class="error-msg text-danger">Please select a preferred contact time.</span>');
                    hasError = true;
                }

                if (!message) {
                    $("#txt_message").after('<span class="error-msg text-danger">Please enter a message.</span>');
                    hasError = true;
                }

                // If there are errors, stop the submission
                if (hasError) {
                    return;
                }

                // Proceed with the AJAX request if validation passes
                $.ajax({
                    type: "POST",
                    url: "default.aspx/SendEmail",
                    data: JSON.stringify({
                        name: name,
                        phone: phone,
                        email: email,
                        service: service,
                        contactTime: contactTime,
                        message: message
                    }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        if (response.d === "success") {
                            alert("Email sent successfully.");

                            // Clear fields on success
                            $("#txtn_name").val('');
                            $("#txtn_phone").val('');
                            $("#txtn_email").val('');
                            $("#ddl_Service").prop('selectedIndex', 0);
                            $("#ddl_CnctTym").prop('selectedIndex', 0);
                            $("#txt_message").val('');

                            // Clear error messages
                            $(".error-msg").remove();
                        } else {
                            alert("Failed to send email.");
                        }
                    },
                    error: function (xhr, status, error) {
                        alert("An error occurred: " + error);
                    }
                });
            });
        });
    </script>

   
    
   


